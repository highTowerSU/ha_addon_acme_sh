{
  "name": "ACME.sh Certs",
  "version": "2.1.0",
  "slug": "acme_sh",
  "description": "Manage certificates with ACME.sh",
  "url": "https://github.com/wernerhp/ha_addon_acme_sh",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "once",
  "boot": "manual",
  "map": ["ssl:rw"],
  "options": {
    "account": "str",
    "server": "str",
    "domains": [null],
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "dns": {}
  },
  "schema": {
    "account": "a@b.de",
    "server": "letsencrypt",
    "domains": ["str"],
    "certfile": "str",
    "keyfile": "str",
    "dns": {
      "provider": "dns_cloudns",
      "env": 
        - CLOUDNS_SUB_AUTH_ID=1234
        - CLOUDNS_AUTH_ID=5678
        - CLOUDNS_AUTH_PASSWORD=aaaa
    }
  }
}
